  <section id="services_section" class="py-5">
      <div class="container">
         <h1 class="text-center">Services we provide</h1>

      <div class="row mt-5">
        {{!-- Card one --}}
        {{#each Servicescrd}}
        <div class="col-md-4">
           <div class="card">
            <div class="card-body">
              <div class="container text-center service_icon py-1 mb-1">
                <i class="{{this.icon}}"></i>
                 </div>

              <h4 class="card-title mb-3 py-1 text-center">{{this.title}}</h4>
              <p class="para-title">{{this.Description}}</p>
                <a href="#">{{this.linktext}}</a>&nbsp;&nbsp;
                 <a href="#">{{this.link}}</a>
             
            </div>
          </div>
        </div>
     {{/each}}
        
      </div>
    </div>
</section>
